{
  "title": "蠢萌小呆瓜的工作日志",
  "description": "蠢萌小呆瓜的工作日志",
  "language": "zh-hans",
  "plugins": [
    "-highlight",
    "-sharing",
    "-fontsettings",
    "sitemap",
    "tbfed-pagefooter",
    "-lunr", 
    "-search", 
    "search-plus",
    "theme-fexa",
    "back-to-top-button",
    "copy-code-button",
    "cuav-chapters",
    "heading-anchors"
  ],
  "variables": {
      "themeFexa":{
          "nav":[
              {
                  "url": "http://books.sangkf.cn",
                  "target":"_blank",
                  "name": "作者博客"
              },
              {
                  "url": "https://github.com/sangskf/it-s-stupid",
                  "target":"_blank",
                  "name": "GitHub"
              }
          ]
      }
  },
  "pluginsConfig": {
    "sitemap": {
      "hostname": "http://books.sangkf.cn/"
    },
    "sharing": {
      "weibo": false,
      "facebook": false,
      "twitter": false,
      "google": false,
      "instapaper": false,
      "vk": false,
      "all": [
          "facebook", "google", "twitter","weibo", "instapaper"
      ]
    },
    "tbfed-pagefooter": {
      "copyright":"Copyright &copy books.sangkf.cn 2019",
      "modify_label": "该文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "theme-fexa": {
      "search-placeholder": "输入关键字搜索"
    },
    "styles" : {
      "website": "styles/website.css"
    }
  }
}